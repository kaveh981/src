{
  "name": "ixm-csv-import-tool",
  "version": "0.1.0",
  "description": "A utility tool to import IXM proposals to System from .csv file",
  "author": "Index Exchange",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "compile": "tsc",
    "copy:static": "cp -rf ./config ./build && cp -rf ./schemas ./build",
    "prebuild": "rm -rf ./build",
    "build": "npm run compile && npm run copy:static",
    "prestart": "npm run build",
    "start": "node ./build/index.js",
    "tslint": "tslint -e./typings/** -e./node_modules/** './**/*.ts'"
  },
  "dependencies": {
    "knex": "0.11.10",
    "mysql": "^2.11.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "csv": "^1.1.0",
    "dotenv": "^2.0.0",
    "js-yaml": "^3.6.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "typings": "^1.4.0",
    "z-schema": "^3.18.0"
  }
}
